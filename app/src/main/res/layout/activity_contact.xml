<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ContactActivity">




    <TextView
        android:id="@+id/national"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="90dp"
        android:layout_marginTop="170dp"
        android:text="@string/National"
        android:textColor="@android:color/background_dark"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/Ekeamap"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="89dp"
        android:layout_marginTop="216dp"
        android:layout_marginEnd="0dp"
        android:text="@string/Address"
        android:textColor="@android:color/background_dark"
        android:textSize="19dp" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/Ekeacall"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="262dp"
        android:layout_marginEnd="12dp"
        android:background="#FFFFFF"
        android:text="Call: +30 213-2146700" />

    <ImageButton
        android:id="@+id/babtn"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="7dp"
        android:layout_marginTop="-2dp"
        android:background="#FFFFFF"
        app:srcCompat="@drawable/back"
        tools:ignore="MissingConstraints,VectorDrawableCompat"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="-2dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="87dp"
        android:layout_height="48dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="207dp"
        app:srcCompat="@drawable/placeholder" />



    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="87dp"
        android:layout_height="48dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="400dp"
        app:srcCompat="@drawable/placeholder" />

    <TextView
        android:id="@+id/EMA"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="89dp"
        android:layout_marginTop="383dp"
        android:text="@string/mitrwo"
        android:textColor="@android:color/background_dark"
        android:textSize="19dp" />

    <TextView
        android:id="@+id/Emamap"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="86dp"
        android:layout_marginTop="428dp"
        android:layout_marginEnd="3dp"
        android:text="@string/address1"
        android:textColor="@android:color/background_dark"
        android:textSize="19dp" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/Emacall"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="480dp"
        android:layout_marginEnd="12dp"
        android:background="#FFFFFF"
        android:text="Call: +30 210-7474274" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="74dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="0dp"
        app:srcCompat="@drawable/images" />

    <TextView
        android:id="@+id/header_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="165dp"
        android:layout_marginTop="62dp"
        android:text="@string/Contact"
        android:textSize="30dp"
        android:textColor="@android:color/holo_red_dark"
        android:textStyle="italic"
        tools:ignore="InvalidId" />


</RelativeLayout>